// SWITCH

var slot = 3;

switch (slot) {
    case 1:
        console.log('Won a TV');
        break;
    case 2:
        console.log('Won a PC');
        break;
    case 3:
        console.log('Won a Car');
        break;
    case 4:
        console.log('Won a Ps');
        break;
    case 5:
        console.log('Won a laptop');
        break; 
    default:
        console.log('Spin again');
}
